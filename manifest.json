{
  "name": "にっけいくんJr",
  "version": "0.0.3",
  "manifest_version": 2,
  "description": "日経平均をカジュアルに取ってきます",
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "web_accessible_resources": [
    "128.png",
    "popup.html"
  ],
  "permissions": [
    "alarms",
    "tabs",
    "notifications",
    "http://indexes.nikkei.co.jp/*",
    "http://www.google.com/calendar/feeds/*"
  ],
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "browser_action": {
    "default_title": "にっけいくんJr",
    "default_icon": "19.png",
    "default_popup": "option.html"
  }
}
